@page
@model CompletedJobsModel
@{
    ViewData["Title"] = "Darbuotojo atlikti darbai";
}
@{
    Layout = "_LayoutWorker";
}


<div class="text-center">
    <h1 class="display-4">Kambario rezervacijos forma</h1>
    <center>
        <form method="post" style="width:50%;">
            <div class="form-group">
                <div class="row">
                    <div class="col-sm-4">
                        <label for="room">Darbuotojo id:</label>
                        <input type="number" name="room" />
                    </div>
                    <div class="col-sm-4">
                        <label for="datefrom">Nuo:</label>
                        <input type="date" name="datefrom" />
                    </div>
                    <div class="col-sm-4">
                        <label for="datetill">Iki:</label>
                        <input type="date" name="datetill" />
                    </div>
                </div>
                <a href="Summary"><input type="button" value="Ieskoti" /></a>
            </div>
        </form>
    </center>
</div>
